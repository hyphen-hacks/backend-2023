// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ParticipantApplication {
  email               String  @id @map("_id")
  firstName           String
  lastName            String
  pronouns            String
  phoneNumber         String
  birthDate           String
  school              String
  gradYear            String
  wantTeam            Boolean
  hasTeam             Boolean
  teamMembers         String
  hasCodingExperience Boolean
  participatedBefore  Boolean
  shirtSize           String
  allergies           String
  comments            String
}
